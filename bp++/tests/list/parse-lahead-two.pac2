module HTTP;

export type Message = unit {
    headers:    list<Header>;
    multipart:  list<BodyLine>;
    : /\n/
        { print self.headers; print self.multipart; }
};

type Header = unit() {
    : /A/;
};

type BodyLine = unit {
    : /B/;
};

