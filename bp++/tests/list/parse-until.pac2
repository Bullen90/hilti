module HTTP;

const LINE = /[^\n]+\n/;
const DASHES = /---\n/;

type Item = unit {
    line:   LINE;
};

export type Test = unit {
    lines: list<Item> &until($$.line == b"---\n")
        { print self.lines; }
    last: LINE;
    };

