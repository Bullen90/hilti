module HTTP;

const LINE = /[^\n]+\n/;
const DASHES = /---\n/;

type Item = unit {
    line:   LINE;
};

export type Test = unit {
    lines: Item[5]
           foreach { print $$; }
    last: LINE;

    on %done {
        print self;
        }

    };

