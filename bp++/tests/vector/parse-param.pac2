module HTTP;

const LINE = /[^\n]+\n/;
const DASHES = /---\n/;

const len = 5;

type Item = unit(t: Test) {
    line: LINE[2]
        { print t.lines; }
};

export type Test = unit {
    lines: Item(self)[2];
    last: LINE;
    on %done {
        print self;
        }
    };


