# @TEST-EXEC:  hilti-build -d -m %INPUT ${PAC_DRIVER} -o a.out
# @TEST-EXEC:  printf '\x01\x02\x03\x04\x01\x02\x03\x04\x01\x02\x03\x04\x05\x06\x07\x08\x09\x10\x11\x12\x13\x14\x15\x16'  | HILTI_DEBUG=binpac ./a.out -p Mini::test >output 2>&1
# @TEST-EXEC:  btest-diff output

module Mini;

import BinPAC;

export type test = unit {
    f1: addr &ipv4;
    f2: addr &ipv4 &byteorder=BinPAC::ByteOrder::Host;
    f3: addr &ipv6;

    on %done {
        print self.f1;
        print self.f2;
        print self.f3;
        }
};
