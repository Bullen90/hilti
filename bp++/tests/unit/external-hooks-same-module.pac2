module Mini;

export type test = unit {

       a: bytes &length=4;
       b: bytes &length=6;

       var foo: bytes;
};

on test::a { print "FOO a", self.a; }
on test::b { print "FOO b", self.b; self.foo = b"x"; }
on test::foo { print "FOO foo", self.foo; }
on test::%init { print "FOO init"; }



