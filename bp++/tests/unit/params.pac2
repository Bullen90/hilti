module HTTP;

export type Foo = unit {
    : /ABC/;
    : Bar(b"XXX");
    : Bar2(b"Y1", b"Y2");
    : Bar3();
    : /GHI/;
    };

type Bar = unit(b: bytes) {
    : /DEF/
        { print b; }
    };

type Bar2 = unit(b1: bytes, b2: bytes) {
    : /DEF/
        { print b1, b2; }
    };

type Bar3 = unit() {
    : /DEF/
        { print b"ok"; }
    };

