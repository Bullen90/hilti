module Main

import Hilti

void run() {
     local ref<bytes> b
     local int<64> i
     
     b = call Hilti::bytes_to_lower(b"AbCdEfG")
     call Hilti::print(b)
     
     b = call Hilti::bytes_to_lower(b"")
     call Hilti::print(b)
     
     i = call Hilti::bytes_to_int(b"1234")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"1")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"-1234")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"-1")
     call Hilti::print(i)

     i = call Hilti::bytes_to_int(b"000")
     call Hilti::print(i)
     
     i = call Hilti::bytes_to_int(b"001")
     call Hilti::print(i)
}     

