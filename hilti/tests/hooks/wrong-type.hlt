module Main

import Hilti

hook void my_hook() {
    call Hilti::print("1st hook function.")
    return.void
}

hook void my_hook(int32 i) {
    call Hilti::print("2nd hook function.")
    return.void
}

hook bool my_hook() {
    call Hilti::print("2nd hook function.")
    hook.stop True
}

void run() {
    hook.run my_hook ()
    return.void
}     

