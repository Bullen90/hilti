module Main

import Hilti

void run() {
     local addr x
     local addr y
     local addr z
     local bool b

     x = 192.168.1.1
     y = 2001:0db8::1428:57ab
     z = ::192.168.1.1

     b = equal x x
     call Hilti::print (b)
     
     b = equal y y
     call Hilti::print (b)
     
     b = equal x y
     call Hilti::print (b)
     
     b = equal x z
     call Hilti::print (b)
     
     b = equal y z
     call Hilti::print (b)

}

