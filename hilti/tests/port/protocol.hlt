# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  test-diff output

module Main

import Hilti

void run() {
     local port a
     local port b
     local Hilti::Protocol f
     local string s

     a = 80/tcp
     f = port.protocol a
     call Hilti::print (a)
     call Hilti::print (f)
     
     b = 53/udp
     f = port.protocol b
     call Hilti::print (b)
     call Hilti::print (f)
     
     s = call Hilti::fmt ("%d %d", (a, b))
     call Hilti::print (s)
}

