module Main

import Hilti

void run() {
     local port x
     local port y
     local port z
     local bool b

     x = 80/tcp
     y = 53/udp
     z = 80/udp

     b = equal x x
     call Hilti::print (b)
     
     b = equal y y
     call Hilti::print (b)
     
     b = equal x y
     call Hilti::print (b)
     
     b = equal x z
     call Hilti::print (b)
}

