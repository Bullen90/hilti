module Main

import Hilti

void run() {
    local string foo = "foo"
    local tuple<int8, int32> t = (1, 2)

    call Hilti::print(foo)
    call Hilti::print(t)

    clear foo
    clear t

    call Hilti::print(foo)
    call Hilti::print(t)
}
