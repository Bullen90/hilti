#
# A bunch of calls with signatures mismatches. These are all supposed to fail.

module Main

import Hilti

void run() {
   local int<32> n
   switch n @default ( (1, @b1), ("bcd", @b2), (3, @b3) )

@b1:
   call Hilti::print (1)
   return.void

@b2:
   call Hilti::print (2)
   return.void

@b3:
   call Hilti::print (3)
   return.void

@default:
   call Hilti::print (-1)
   return.void
}

