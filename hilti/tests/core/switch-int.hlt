# @TEST-EXEC:  btest-diff output

module Main

import Hilti


void doSwitch(int<32> n) {

   switch n @default ( (1, @b1), (2, @b2), (3, @b3) )
   
@b1:
   call Hilti::print (1)
   return.void
   
@b2:
   call Hilti::print (2)
   return.void

@b3:
   call Hilti::print (3)
   return.void
   
@default:   
   call Hilti::print (-1)
   return.void
}     

void run() {
   call doSwitch(3)
   call doSwitch(2)
   call doSwitch(1)
   call doSwitch(255)
   }
