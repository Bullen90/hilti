module Main

import Hilti

void run() {
     local net a
     local int8 f

     a = 192.168.1.0/24
     f = net.length a
     call Hilti::print (a)
     call Hilti::print (f)

     a = 2001:0db8::1428:57ab/48
     f = net.length a
     call Hilti::print (a)
     call Hilti::print (f)

     a = ::192.168.1.0/120
     f = net.length a
     call Hilti::print (a)
     call Hilti::print (f)
}

