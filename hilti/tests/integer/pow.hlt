module Main

import Hilti

void run() {
    local int<64> i
    local int<16> i2

    local int<8> i8a = 2
    local int<8> i8b = 3

    i = int.pow 2 9
    call Hilti::print (i)

    i2 = int.pow i8a i8b
    call Hilti::print (i2)
}

