# @TEST-EXEC:  test-diff output

module Main

import hilti

void run() {
   local bool cond
   cond = int.lt 1 2 
   if.else cond @yes @no
   
@yes:
   call Hilti::print (1)
   return.void

@no:
   call Hilti::print (0)
   return.void
}     

