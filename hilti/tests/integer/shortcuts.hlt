#
# Check whether int16, int32, etc. turn into the same as int<n>. 

module Main

void run() {
    local int8 i1
    local int8 i2
    local int<8> i3
    
    i1 = int.add 5 5 
    i2 = int.add 10 10
    i3 = int.add i1 i2
}

