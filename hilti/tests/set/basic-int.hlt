module Main

import Hilti

void run() {
    local bool b
    local ref<set<int32>> m

    m = new set<int32>

    set.insert m 10
    set.insert m 20
    call Hilti::print(m)

    b = set.exists m 10
    call Hilti::print (b)

    b = set.exists m 20
    call Hilti::print (b)

}
