# @TEST-EXEC:  hilti-build2 %INPUT -o a.out
# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  btest-diff output

module Main

import Hilti

void run() {
    local bool b
    local int32 s
    local ref<timer_mgr> t
    local ref<set<string>> m

    t = new timer_mgr
    m = new set<string> t
    set.timeout m Hilti::ExpireStrategy::Create 20.0

    set.insert m "A-0"
    set.insert m "B-0"

    timer_mgr.advance t 5.0
    set.insert m "C-5"
    set.insert m "D-5"

    # This shouldn't have any effect on the expiration.
    b = set.exists m "B-0"

    timer_mgr.advance t 10.0
    set.insert m "E-10"
    set.insert m "F-10"

    call Hilti::print(m)

    call Hilti::print("Advance to 10")
    timer_mgr.advance t 10.0
    call Hilti::print(m)

    call Hilti::print("Advance to 20")
    timer_mgr.advance t 20.0
    call Hilti::print(m)

    call Hilti::print("Advance to 25")
    timer_mgr.advance t 25.0
    call Hilti::print(m)

    call Hilti::print("Advance to 50")
    timer_mgr.advance t 50.0
    call Hilti::print(m)

    call Hilti::print(t)
}
