module Main

import Hilti

void run() {
    local bool b
    local ref<set<tuple<string,int64>>> m
    
    m = new set<tuple<string,int64>>
    
    set.insert m ("Foo",1)
    set.insert m ("Bar",2)
    call Hilti::print(m)

    b = set.exists m ("Foo",1)
    call Hilti::print (b)
    
    b = set.exists m ("Bar",2)
    call Hilti::print (b)
}
