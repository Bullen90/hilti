module Main

import Hilti

void run() {
    local bool b
    local int64 i
    local ref<set<string>> m

    m = new set<string>
    call Hilti::print(m)

    set.insert m "Foo"
    set.insert m "Bar"

    i = set.size m
    call Hilti::print(i)
    call Hilti::print(m)

    set.remove m "Foo"
    set.remove m "Bar"

    i = set.size m
    call Hilti::print(i)
    call Hilti::print(m)

    b = set.exists m "Foo"
    call Hilti::print(b)
    b = set.exists m "Bar"
    call Hilti::print(b)

    set.insert m "Foo"
    set.insert m "Bar"

    i = set.size m
    call Hilti::print(i)
    call Hilti::print(m)

    b = set.exists m "Foo"
    call Hilti::print(b)
    b = set.exists m "Bar"
    call Hilti::print(b)

    set.clear m

    i = set.size m
    call Hilti::print(i)
    call Hilti::print(m)

}
