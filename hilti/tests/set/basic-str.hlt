module Main

import Hilti

void run() {
    local bool b
    local ref<set<string>> m
    
    m = new set<string>
    
    set.insert m "Foo"
    set.insert m "Bar"
    call Hilti::print (m)
    
    b = set.exists m "Foo"
    call Hilti::print (b)
    
    b = set.exists m "Bar"
    call Hilti::print (b)
}
