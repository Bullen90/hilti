# @TEST-EXEC:  hilti-build2 %INPUT -o a.out
# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  btest-diff output

module Main

import Hilti

void run() {
    local int32 s
    local bool b
    local ref<timer_mgr> t
    local ref<set<string>> m

    t = new timer_mgr
    m = new set<string> t
    set.timeout m Hilti::ExpireStrategy::Access interval(20.0)

    set.insert m "A-0"
    set.insert m "B-0"

    timer_mgr.advance t time(5.0)
    set.insert m "C-5"
    set.insert m "D-5"

    timer_mgr.advance t time(10.0)
    set.insert m "E-10"
    set.insert m "F-10"

    call Hilti::print(m)
    call Hilti::print("")

    timer_mgr.advance t time(10.0)
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

    b = set.exists m "B-0"

    timer_mgr.advance t time(20.0)
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

    timer_mgr.advance t time(25.0)
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

    set.remove m "F-10"
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

    timer_mgr.advance t time(50.0)
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

}
