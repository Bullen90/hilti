# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  test-diff output

module Main

import Hilti

void run() {
    local string i
    local ref<set<string>> m

    local bool eq
    local iterator<set<string>> cur
    local iterator<set<string>> last

    m = new set<string>
    
    set.insert m "AAA"
    set.insert m "BBB"
    set.insert m "CCC"
    set.insert m "DDD"
    set.insert m "EEE"
    set.insert m "FFF"
    
    cur = begin m
    last = end m

@loop:
    eq = equal cur last
    if.else eq @exit @cont
    
@cont:
    i = deref cur
    call Hilti::print (i)
    cur = incr cur
    jump @loop
    
@exit: return.void
    }

