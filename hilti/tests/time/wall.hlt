#
# Can't really check the output of time.wall so we just make sure it
# compiles and outputs something ...
#
module Main

import Hilti

void run() {
    local time t
    local string s

    t = time.wall

    s = call Hilti::fmt("d:%f %s", (t, t))
    call Hilti::print (s)
}

