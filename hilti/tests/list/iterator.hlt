module Main

import Hilti

void run() {
    local int32 i
    local int64 s
    local ref<list<int32>> l

    local bool eq
    local iterator<list<int32>> cur
    local iterator<list<int32>> last

    l = new list<int32>
    list.push_back l 10
    list.push_back l 20
    list.push_back l 30
    list.push_back l 40
    list.push_back l 50

    cur = begin l
    last = end l

@loop:
    eq = equal cur last
    if.else eq @exit @cont
    
@cont:
    i = deref cur
    call Hilti::print (i)
    cur = incr cur
    jump @loop
    
@exit: return.void
    }
