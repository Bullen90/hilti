# @TEST-EXEC:  test-diff output

module Main

import Hilti

void run() {
    local int32 i
    local int64 s
    local ref<vector<int32>> v

    local bool eq
    local iterator<vector<int32>> cur
    local iterator<vector<int32>> end

    v = new vector<int32>
    vector.set v 0 10
    vector.set v 2 11
    vector.set v 4 12

    cur = vector.begin v
    end = vector.end v

@loop:
    eq = equal cur end
    if.else eq @exit @cont
    
@cont:
    i = deref cur
    call Hilti::print (i)
    cur = incr cur
    jump @loop
    
@exit: return.void
    }
