module Main

import Hilti

void run() {
    local int32 i
    local int64 s
    local ref<vector<int32>> v

    v = new vector<int32>

    vector.push_back v 10
    vector.push_back v 20
    vector.push_back v 30

    i = vector.get v 0
    call Hilti::print(i)
    i = vector.get v 1
    call Hilti::print(i)
    i = vector.get v 2
    call Hilti::print(i)

    s = vector.size v
    call Hilti::print(s)
}
