# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  btest-diff output
#

module Main

import Hilti

global ref<bytes> b1 = b"123"
global ref<bytes> b2 = b"123"
global ref<bytes> b3 = b"abc"
global ref<bytes> b4 = b"12"
global ref<bytes> b5 = b"1234"
global ref<bytes> b6 = b""
global ref<bytes> b7 = b""

void run() {

    local int<8> i
    local bool b

    i = bytes.cmp b1 b2
    call Hilti::print (i)
    
    i = bytes.cmp b1 b3
    call Hilti::print (i)

    i = bytes.cmp b1 b4
    call Hilti::print (i)

    i = bytes.cmp b1 b5
    call Hilti::print (i)
    
    i = bytes.cmp b1 b6
    call Hilti::print (i)
    
    i = bytes.cmp b6 b1
    call Hilti::print (i)
    
    i = bytes.cmp b6 b7
    call Hilti::print (i)
    
    b = equal b1 b2
    call Hilti::print (b)
    
    b = equal b1 b3
    call Hilti::print (b)
    
}
