module Foo

import Hilti

export int<32> test() {
    call Hilti::print("1")
    yield
    call Hilti::print("2")
    yield
    call Hilti::print("3")
    yield
    call Hilti::print("4")
    yield
    call Hilti::print("5")
    
    return.result 42
}


