module Foo

import Hilti

void test(int<64> i) {
    call Hilti::print(i, False)
    call Hilti::print(" ", False)
    call Hilti::print("1")
    yield
    call Hilti::print(i, False)
    call Hilti::print(" ", False)
    call Hilti::print("2")
    yield
    call Hilti::print(i, False)
    call Hilti::print(" ", False)
    call Hilti::print("3")
    yield
    call Hilti::print(i, False)
    call Hilti::print(" ", False)
    call Hilti::print("4")
    yield
    call Hilti::print(i, False)
    call Hilti::print(" ", False)
    call Hilti::print("5")
}

export test

