module Main

import Hilti

global ref<regexp> g = /ghi/

void run() {
    local ref<regexp> re
    re = /abc/
    call Hilti::print(re)
    call Hilti::print(/def/)
    call Hilti::print(/ghi/)
    call Hilti::print(g)
}
