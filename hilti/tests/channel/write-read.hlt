# @TEST-EXEC:  hilti-build %INPUT -o a.out 
# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  test-diff output

module Main

import Hilti

void run() {
    local ref<channel<int32,0>> ch
    ch = channel.new int32 0
    local int32 a
    local int32 b
    local int32 c

    a = int.mul 7 6
    b = int.add 9 6
    c = int.sub 50 8

    channel.write ch a
    channel.write ch b
    channel.write ch c

    local int32 x
    local int32 y
    local int32 z

    x = channel.read ch
    y = channel.read ch
    z = channel.read ch

    call Hilti::print(x)
    call Hilti::print(y)
    call Hilti::print(z)
}
