# @TEST-EXEC:  hilti-build %INPUT -o a.out
# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  btest-diff output

module Main

import Hilti

void run() {
    local ref<channel<int64>> ch
    ch = new channel<int64> 2

    local int64 a
    local int64 b
    local int64 c
    local int64 d

    a = int.mul 7 6
    b = int.add 9 6
    c = int.sub 50 8

    channel.write_try ch a
    channel.write_try ch b
    channel.write_try ch c
    channel.write_try ch d

    d = channel.size ch
    call Hilti::print(d)

}
