module Main

import Hilti

void foo(int32 n) {
    call Hilti::print ("Yippie!")
    call Hilti::print (n)
}

void run() {

    local ref<timer> t
    local ref<timer_mgr> mgr

    mgr = new timer_mgr
    t = new timer foo (42)

    timer_mgr.schedule mgr 10.0 t

    timer_mgr.advance mgr 20.0

    return.void
}


