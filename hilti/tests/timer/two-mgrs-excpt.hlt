module Main

import Hilti

void foo(int32 n) {
    call Hilti::print ("Yippie!")
    call Hilti::print (n)
}

void run() {

    local ref<timer> t
    local ref<timer_mgr> mgr
    local ref<timer_mgr> mgr2
    
    mgr = new timer_mgr 
    t = new timer foo (42)

    timer_mgr.schedule mgr 10.0 t
    timer_mgr.schedule mgr2 10.0 t

    call Hilti::print("not reached")

    return.void
}     


