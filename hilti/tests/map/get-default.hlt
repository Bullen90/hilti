module Main

import Hilti

void run() {
    local int32 v
    local ref<map<string, int32>> m

    m = new map<string, int32>

    map.insert m "Foo" 10
    map.insert m "Bar" 20

    v = map.get_default m "Foo" 42
    call Hilti::print(v)

    v = map.get_default m "FooBar" 42
    call Hilti::print(v)
}
