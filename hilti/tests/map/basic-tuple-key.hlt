# @TEST-EXEC:  test-diff output

module Main

import Hilti

void run() {
    local int32 v
    local ref<map<tuple<string,int64>, int32>> m
    
    m = new map<tuple<string,int64>, int32>
    
    map.insert m ("Foo",1) 10
    map.insert m ("Bar",2) 20
    call Hilti::print(m)
    
    v = map.get m ("Foo",1)
    call Hilti::print(v)
    
    v = map.get m ("Bar",2)
    call Hilti::print(v)
}
