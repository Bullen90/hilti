module Main

import Hilti

void run() {
    local tuple<string,int64> v
    local ref<map<int32,tuple<string,int64>>> m

    m = new map<int32,tuple<string,int64>>

    map.insert m 10 ("Foo",1)
    map.insert m 20 ("Bar",2)
    call Hilti::print(m)

    v = map.get m 10
    call Hilti::print(v)

    v = map.get m 20
    call Hilti::print(v)
}
