module Main

import Hilti

void run() {
    local string v
    local ref<map<int32, string>> m

    m = new map<int32, string>

    map.insert m 10 "Foo"
    map.insert m 20 "Bar"

    v = map.get_default m 10 "April April"
    call Hilti::print(v)

    v = map.get_default m 30 "April April"
    call Hilti::print(v)
}
