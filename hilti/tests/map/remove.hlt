module Main

import Hilti

void run() {
    local bool b
    local int32 i
    local ref<map<string, int32>> m

    m = new map<string, int32>
    call Hilti::print(m)

    map.insert m "Foo" 10
    map.insert m "Bar" 20

    i = map.size m
    call Hilti::print(i)
    call Hilti::print(m)

    map.remove m "Foo"
    map.remove m "Bar"

    i = map.size m
    call Hilti::print(i)
    call Hilti::print(m)

    b = map.exists m "Foo"
    call Hilti::print(b)
    b = map.exists m "Bar"
    call Hilti::print(b)

    map.insert m "Foo" 10
    map.insert m "Bar" 20

    i = map.size m
    call Hilti::print(i)
    call Hilti::print(m)

    b = map.exists m "Foo"
    call Hilti::print(b)
    b = map.exists m "Bar"
    call Hilti::print(b)

    map.clear m

    i = map.size m
    call Hilti::print(i)
    call Hilti::print(m)

}
